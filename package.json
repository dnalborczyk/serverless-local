{
  "name": "serverless-offline",
  "version": "6.0.0-alpha.67",
  "description": "Emulate AWS λ and API Gateway locally when developing your Serverless project",
  "license": "MIT",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "rimraf dist && babel src --extensions '.ts' --ignore \"**/__tests__/**/*\" --out-dir dist && copyfiles -u 1 \"src/**/*.{js,vm,py,rb}\" dist",
    "lint": "eslint .",
    "list-contributors": "echo 'clone https://github.com/mgechev/github-contributors-list.git first, then run npm install' && cd ../github-contributors-list && node bin/githubcontrib --owner dherault --repo serverless-offline --sortBy contributions --showlogin true --sortOrder desc > contributors.md",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run build && npm test",
    "prettier": "prettier --check .",
    "test": "npm run build && jest --verbose --silent --runInBand",
    "test:cov": "npm run build && jest --coverage --silent --runInBand --collectCoverageFrom=src/**/*.js",
    "test:log": "npm run build && jest --verbose",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dherault/serverless-offline.git"
  },
  "bugs": {
    "url": "https://github.com/dherault/serverless-offline/issues"
  },
  "homepage": "https://github.com/dherault/serverless-offline",
  "keywords": [
    "serverless",
    "serverless framework",
    "serverless local",
    "serverless offline",
    "serverless plugin",
    "aws",
    "amazon web services",
    "api gateway",
    "http",
    "lambda",
    "schedule",
    "websocket"
  ],
  "files": [
    "dist/**",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "author": "David Hérault <dherault@gmail.com> (https://github.com/dherault)",
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.11",
    "@hapi/h2o2": "^8.3.2",
    "@hapi/hapi": "^18.4.1",
    "boxen": "^4.2.0",
    "chalk": "^4.0.0",
    "cuid": "^2.1.8",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "js-string-escape": "^1.0.1",
    "jsonpath-plus": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.3.0",
    "luxon": "^1.23.0",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "object.fromentries": "^2.0.2",
    "p-memoize": "^4.0.0",
    "p-queue": "^6.3.0",
    "p-retry": "^4.2.0",
    "please-upgrade-node": "^3.2.0",
    "portfinder": "^1.0.25",
    "semver": "^7.2.2",
    "update-notifier": "^4.1.0",
    "velocityjs": "^2.0.0",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-dynamic-import": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@types/fs-extra": "^8.1.0",
    "@types/hapi__boom": "^7.4.1",
    "@types/hapi__h2o2": "^8.3.1",
    "@types/hapi__hapi": "^18.2.6",
    "@types/js-string-escape": "^1.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/jszip": "^3.1.7",
    "@types/luxon": "^1.22.0",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.6",
    "@types/node-schedule": "^1.3.0",
    "@types/semver": "^7.1.0",
    "@types/serverless": "^1.18.6",
    "@types/update-notifier": "^4.1.0",
    "@types/ws": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "aws-sdk": "^2.656.0",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "p-map": "^4.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "serverless": "^1.67.3",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "serverless": ">=1.60.0"
  }
}
